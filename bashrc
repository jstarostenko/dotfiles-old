alias g='git status'
alias gc='git commit -m'
alias gca='git commit -am'
alias gd='git diff'
alias ga='git add'

alias sl='ls'
alias untar='tar -xvf'
alias exe='chmod +x'
alias zcp="rsync --rsh='ssh' -avz --progress --partial"
alias zmv="rsync --remove-source-files --rsh='ssh' -avz --progress --partial"
alias code="cd code"
